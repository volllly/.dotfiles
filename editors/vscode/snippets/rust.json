{
	"Derive": {
		"prefix": "derive",
		"body": "#[derive($1)]"
	},
	"Attribute": {
		"prefix": "attribute",
		"body": "#[$1]"
	},
	"Method": {
		"prefix": "method",
		"body": ".$1($2)"
	},
	"Method with closure": {
		"prefix": "method-closure",
		"body": ".$1(|$2| $3)"
	},
	"into Ok": {
		"prefix": ".ok",
		"body": ".into_ok()"
	},
	"into Some": {
		"prefix": ".some",
		"body": ".into_some()"
	},
	"dbg": {
		"prefix": "dbg",
		"description": "",
		"scope": "",
		"body": [
			"dbg!($1);"
		]
	},
	"inspect Print": {
		"prefix": "inspect",
		"body": [
			".inspect(|d| { dbg!(d); })"
		]
	},
}