{
	"Derive": {
		"prefix": "derive",
		"body": "#[derive($1)]"
	},
	"Attribute": {
		"prefix": "attribute",
		"body": "#[$1]"
	},
	"Method": {
		"prefix": "method",
		"body": ".$1($2)"
	},
	"Method with closure": {
		"prefix": "method-closure",
		"body": ".$1(|$2| $3)"
	},
	"pipe": {
		"prefix": "pipe",
		"body": ".pipe($1)"
	},
	"pipe Ok": {
		"prefix": "pipe-ok",
		"body": ".pipe(Ok)"
	},
	"pipe Some": {
		"prefix": "pipe-some",
		"body": ".pipe(Some)"
	},
	"pipe Error": {
		"prefix": "pipe-err",
		"body": ".pipe(Err)"
	},
	"into Ok": {
		"prefix": ".ok",
		"body": ".into_ok()"
	},
	"into Some": {
		"prefix": ".some",
		"body": ".into_some()"
	},
	"Debug Print": {
		"prefix": "debug",
		"body": "println!(\"{:?}\", $1);"
	},
	"inspect Print": {
		"prefix": "inspect",
		"body": ".inspect(|d| println!(\"{:?}\", d))"
	},
}