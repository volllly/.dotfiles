{
    "editor.tabSize": 2,
    "editor.fontFamily": "PragmataPro Liga",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "debug.toolBarLocation": "docked",
    "editor.minimap.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "html.format.indentInnerHtml": true,
    "editor.renderWhitespace": "boundary",
    "editor.renderControlCharacters": true,
    "explorer.confirmDelete": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "debug.console.fontFamily": "PragmataPro Liga",
    "scm.defaultViewMode": "tree",
    "omnisharp.useEditorFormattingSettings": false,
    "editor.definitionLinkOpensInPeek": true,
    "gruvboxMaterial.darkPalette": "original",
    "workbench.editorAssociations": {
        "git-rebase-todo": "default"
    },
    "debug.allowBreakpointsEverywhere": true,
    "editor.bracketPairColorization.enabled": true,
    "csharp.semanticHighlighting.enabled": true,
    "redhat.telemetry.enabled": false,
    "gitmoji.outputType": "code",
    "gitmoji.showEmojiCode": true,
    "projectManager.any.maxDepthRecursion": 1,
    "projectManager.git.baseFolders": [
        "~/source"
    ],
    "projectManager.git.maxDepthRecursion": 1,
    "projectManager.ignoreProjectsWithinProjects": true,
    "githubPullRequests.fileListLayout": "tree",
    "terminal.integrated.fontSize": 15,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "githubPullRequests.createOnPublishBranch": "never",
    "editor.formatOnSave": true,
    "window.commandCenter": true,
    "githubPullRequests.pullBranch": "never",
    "workbench.colorTheme": "Gruvbox Material Dark",
    "workbench.list.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.smoothScrolling": true,
    "terminal.integrated.smoothScrolling": true,
    "[markdown]": {
        "editor.wordWrap": "off"
    },
    "git.mergeEditor": true,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "terminal.integrated.defaultProfile.windows": "nu",
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.external.windowsExec": "wt.exe",
    "terminal.integrated.profiles.windows": {
        "nu": {
            "path": "nu",
            "icon": "chevron-right"
        },
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        }
    },
    "window.menuBarVisibility": "compact",
    "vspacecode.bindingOverrides": [
        {
            "keys": "j.l",
            "name": "Jump to line",
            "type": "command",
            "icon": "list-flat",
            "command": "workbench.action.gotoLine"
        },
        {
            "keys": "f.O",
            "name": "Open folder",
            "type": "command",
            "icon": "folder-opened",
            "command": "workbench.action.files.openFolder"
        },
        {
            "keys": "p.p",
            "name": "+Switch project",
            "type": "command",
            "icon": "project",
            "command": "projectManager.listProjects",
        },
        {
            "keys": "p.l",
            "position": -1,
        },
        {
            "keys": "p.r",
            "name": "Refresh projects",
            "type": "command",
            "icon": "refresh",
            "command": "projectManager.refreshProjects",
        },
        {
            "keys": "p.l",
            "name": "+Recent projects",
            "type": "command",
            "icon": "project",
            "command": "workbench.action.openRecent",
        },
        {
            "keys": "T.B",
            "name": "Toggle secondary sidebar visibility",
            "type": "command",
            "icon": "split-horizontal",
            "command": "workbench.action.toggleAuxiliaryBar",
        },
        {
            "keys": "f.p",
            "name": "Fuzzy finder",
            "type": "command",
            "icon": "search",
            "command": "workbench.action.quickOpen"
        },
        {
            "keys": "!",
            "name": "Close Terminal",
            "type": "command",
            "icon": "terminal",
            "command": "workbench.action.terminal.toggleTerminal",
            "when": "terminalFocus"
        },
        {
            "keys": "b.o",
            "name": "Keep Open",
            "type": "command",
            "icon": "lock-small",
            "command": "workbench.action.keepEditor",
        },
        {
            "keys": "g.b",
            "name": "+Branches",
            "type": "command",
            "icon": "git-branch",
            "command": "magit.branching"
        },
        {
            "keys": "g.s",
            "name": "+Sync",
            "type": "bindings",
            "icon": "git-branch",
            "bindings": [
                {
                    "key": "p",
                    "name": "Push",
                    "type": "command",
                    "icon": "repo-push",
                    "command": "magit.pushing"
                },
                {
                    "key": "u",
                    "name": "Pull",
                    "type": "command",
                    "icon": "repo-pull",
                    "command": "magit.pulling"
                },
            ]
        },
        {
            "keys": "g.c",
            "name": "+Commits",
            "type": "commands",
            "icon": "git-commit",
            "command": "magit.commit"
        },
        {
            "keys": "g.i",
            "position": -1,
        },
        {
            "keys": "g.S",
            "name": "+Stashes",
            "type": "command",
            "icon": "list-flat",
            "command": "magit.stashing"
        },
        {
            "keys": "g.U",
            "position": -1,
        },
        {
            "keys": "g.m",
            "position": -1,
        },
        {
            "keys": "B",
            "name": "+Bookmarks",
            "type": "bindings",
            "icon": "bookmark",
            "bindings": [
                {
                    "key": "B",
                    "name": "Toggle",
                    "type": "command",
                    "icon": "bookmark",
                    "command": "bookmarks.toggle"
                },
                {
                    "key": "b",
                    "name": "Toggle Labled",
                    "type": "command",
                    "icon": "bookmark",
                    "command": "bookmarks.toggleLabeled"
                },
                {
                    "key": "l",
                    "name": "List",
                    "type": "command",
                    "icon": "list-flat",
                    "command": "bookmarks.list"
                },
                {
                    "key": "L",
                    "name": "List All",
                    "type": "command",
                    "icon": "list-tree",
                    "command": "bookmarks.listFromAllFiles"
                },
                {
                    "key": "n",
                    "name": "Next",
                    "type": "command",
                    "icon": "goto-next-location",
                    "command": "bookmarks.jumpToNext"
                },
                {
                    "key": "p",
                    "name": "Previous",
                    "type": "command",
                    "icon": "goto-previous-location",
                    "command": "bookmarks.jumpToPrevious"
                },
                {
                    "key": "c",
                    "name": "Clear",
                    "type": "command",
                    "icon": "notifications-clear",
                    "command": "bookmarks.clear"
                },
                {
                    "key": "C",
                    "name": "Clear All",
                    "type": "command",
                    "icon": "clear-all",
                    "command": "bookmarks.clearFromAllFiles"
                },
            ]
        },
        {
            "keys": "\\",
            "name": "+Copilot",
            "type": "bindings",
            "icon": "github",
            "bindings": [
                {
                    "key": "\\",
                    "name": "Trigger nline",
                    "type": "command",
                    "icon": "lightbulb",
                    "command": "editor.action.inlineSuggest.trigger"
                },
                {
                    "key": "c",
                    "name": "Trigger",
                    "type": "command",
                    "icon": "lightbulb-autofix",
                    "command": "github.copilot.interactiveEditor.generate"
                },
                {
                    "key": "C",
                    "name": "Chat",
                    "type": "command",
                    "icon": "comment-discussion",
                    "command": "workbench.panel.chat.view.copilot.focus"
                },
                {
                    "key": "q",
                    "name": "Question",
                    "type": "command",
                    "icon": "question",
                    "command": "chat.action.askQuickQuestion",
                },
                {
                    "key": "e",
                    "name": "Explain",
                    "type": "command",
                    "icon": "lightbulb",
                    "command": "github.copilot.interactiveEditor.explain"
                },
                {
                    "key": "g",
                    "name": "Enable/Disable",
                    "type": "command",
                    "icon": "circle-large-outline",
                    "command": "github.copilot.toggleCopilot",
                },
                {
                    "key": "x",
                    "name": "Clear Chat",
                    "type": "command",
                    "icon": "clear-all",
                    "command": "workbench.action.chat.copilot.clear"
                }
            ]
        }
    ],
    "whichkey.sortOrder": "nonNumberFirst",
    "editor.accessibilitySupport": "off",
    "rust-analyzer.check.command": "clippy",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "[python]": {
        "editor.formatOnType": true
    },
    "terminal.integrated.automationProfile.windows": {
        "path": "cmd",
        "args": []
    },
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false
    },
    "editor.inlineSuggest.showToolbar": "always",
    "editor.inlineSuggest.enabled": true,
    "git.openRepositoryInParentFolders": "never",
    "diffEditor.ignoreTrimWhitespace": true,
    "gruvboxMaterial.lightPalette": "original",
    "csharp.debug.justMyCode": false,
    "github.copilot.editor.enableAutoCompletions": false,
    "gruvboxMaterial.darkWorkbench": "high-contrast",
    "vs-kubernetes": {
        "vs-kubernetes.crd-code-completion": "disabled"
    },
    "yaml.schemaStore.enable": false,
}